@model IdentityUser

<h2>Xoá User</h2>

<div>
    <h4>Xác nhận xoá?</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2" style="padding-bottom:10px">Username</dt>
        <dd class="col-sm-10">@Model.UserName</dd>
        <dt class="col-sm-2" style="padding-bottom:10px">Email</dt>
        <dd class="col-sm-10">@Model.Email</dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Xoá" class="btn btn-danger" /> | 
        <partial name="Btn-Back" />
    </form>
</div>


